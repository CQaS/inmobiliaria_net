@model AplicacionPrueba.Models.Inquilino

@{
    ViewData["Title"] = "Altas";
}

<h1>Altas de Inquilinos</h1>

<div class="container">
    <div>
        <form method="post" asp-action="Alta">
            <div class="form-group">
                <label for="dni_id" class="control-label">DNI</label>
                <input type="text" class="form-control" id="dni_id" name="dni" placeholder="Dni">
            </div>
            <div class="form-group">
                <label for="name_id" class="control-label">Nombre</label>
                <input type="text" class="form-control" id="name_id" name="nombre" placeholder="Nombre">
            </div>
            <div class="form-group">
                <label for="mail" class="control-label">Mail</label>
                <input type="text" class="form-control" id="mail" name="mail" placeholder="E-Mail">
            </div>
            <div class="form-group">
                <label for="dire" class="control-label">Direccion</label>
                <input type="text" class="form-control" id="direccion" name="direccion" placeholder="Dieccion">
            </div>
            <input type="submit" value="Confirmar" />
        </form>
    </div>
</div>
